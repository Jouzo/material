{
  "name": "material",
  "version": "0.1.2",
  "description": "Vanilla Implementation of Material Design Components",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/floor/material.git"
  },
  "author": "Code Politan <codepolitan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floor/material/issues"
  },
  "homepage": "https://github.com/floor/material",
  "keywords": [
    "material"
  ],
  "dependencies": {
    "moment": "^2.18.1",
    "morpheus": "^0.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^14.4.0",
    "exorcist": "^0.4.0",
    "jsdoc": "^3.5.4",
    "require-dir": "^0.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "build-js": "browserify src/index.js  --debug -o 'exorcist dist/material.js.map > dist/material.js' -v",
    "build-css": "lessc src/skin/material/material.less > dist/material.css",
    "watch": "npm run watch-js & npm run watch-css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
